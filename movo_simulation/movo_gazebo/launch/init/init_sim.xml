<launch>	
    <node pkg="movo_gazebo" type="init_sim" name="init_sim" output="log">
	<param if="$(optenv MOVO_HAS_NO_RIGHT_ARM false)" name="jaco_dof_right_arm" value="none" />
	<param if="$(optenv MOVO_HAS_RIGHT_ARM_6DOF false)" name="jaco_dof_right_arm" value="6dof" />
	<param if="$(optenv MOVO_HAS_RIGHT_ARM_7DOF false)" name="jaco_dof_right_arm" value="7dof" />
        <param if="$(optenv MOVO_HAS_NO_LEFT_ARM false)" name="jaco_dof_left_arm" value="none" />
	<param if="$(optenv MOVO_HAS_LEFT_ARM_6DOF false)" name="jaco_dof_left_arm" value="6dof" />
	<param if="$(optenv MOVO_HAS_LEFT_ARM_7DOF false)" name="jaco_dof_left_arm" value="7dof" />
    </node>
</launch>


