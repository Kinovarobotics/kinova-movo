<?xml version="1.0"?>
<launch>

	<param name="use_sim_time" value="true" />

    <!-- ROS Driver, Robot State Publisher, and sensors -->
    <node pkg="si_utils" type="timed_roslaunch"
        args="1 movo_gazebo movo_test_zone.launch"
        name="movo_gazebo_bringup" output="screen"/>
        
    <!-- MoveIt! configuration -->
    <node pkg="si_utils" type="timed_roslaunch"
        args="20 movo_moveit_config movo_moveit_planning_execution.launch sim:=true"
        name="movo_moveit_bringup" output="log"/>

    <!-- Kinematics test -->
    <node pkg="si_utils" type="timed_roslaunch"
        args="30 movo_gazebo init_demo_sim.launch"
        name="movo_demo_init" output="log"/>
        
        

</launch>
