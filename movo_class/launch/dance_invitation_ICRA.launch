<launch>
<!-- Activate face_tracking -->
	<node pkg="si_utils" type="timed_roslaunch" 
            args="1 movo_demos face_tracking.launch" 
            name="face_tracking" output= "screen"/>
<!-- Move to pick hand pose -->
    <node pkg="si_utils" type="timed_roslaunch"
          args="3 movo_demos follow_me_pose.launch"
          name="follow_me_pose" output="screen"/>

    <!-- Enable follow_me feature -->
    <node pkg="si_utils" type="timed_roslaunch"
          args="11 movo_demos follow_me_activation.launch"
          name="follow_me_activation" output="screen"/>

</launch>
